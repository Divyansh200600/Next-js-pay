{
    "version": 2,
    "builds": [
      { "src": "server/index.js", "use": "@vercel/node" },
      { "src": "next.config.js", "use": "@vercel/next" }
    ],
    "routes": [
      { "src": "/api/createOrder", "dest": "server/index.js" },
      { "src": "/(.*)", "dest": "next.config.js" }
    ]
  }
  